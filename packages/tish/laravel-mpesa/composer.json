{
    "name": "tish/laravel-mpesa",
    "description": "Laravel M-Pesa Lipa Na M-Pesa integration package",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Tish\\LaravelMpesa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tish\\LaravelMpesa\\MpesaServiceProvider"
            ],
            "aliases": {
                "Mpesa": "Tish\\LaravelMpesa\\Facades\\Mpesa"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
